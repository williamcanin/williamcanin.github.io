{
  "name": "rawfeed",
  "version": "0.1.2",
  "description": "A raw Jekyll theme for minimalists",
  "scripts": {
    "install": "echo 'wait...' && bundle install",
    "build": "cross-env JEKYLL_ENV=production bundle exec jekyll build && gulp",
    "serve": "JEKYLL_ENV=development; echo JEKYLL_ENV=$JEKYLL_ENV; bundle exec jekyll serve --drafts --livereload",
    "draft": "bundle exec rake create:draft",
    "posts": "bundle exec rake move:posts",
    "page": "bundle exec rake create:page",
    "resume": "bundle exec rake create:resume",
    "home:about": "bundle exec rake home:about",
    "home:blog": "bundle exec rake home:blog",
    "blog:disable": "bundle exec rake blog:disable",
    "blog:enable": "bundle exec rake blog:enable",
    "clean": "gulp clean",
    "help": "gulp menu --silent"
  },
  "repository": "git+https://github.com/williamcanin/rawfeed.git",
  "keywords": [
    "rawfeed",
    "theme",
    "template",
    "jekyll",
    "retro"
  ],
  "author": "William C. Canin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/williamcanin/rawfeed/issues"
  },
  "homepage": "https://github.com/williamcanin/rawfeed#readme",
  "devDependencies": {
    "cross-env": "^10.0.0",
    "del": "^8.0.1",
    "gulp": "^5.0.1",
    "gulp-cli": "^3.1.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-uglify": "^3.0.2",
    "kleur": "^4.1.5"
  }
}
